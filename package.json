{
  "name": "lighthouse",
  "version": "1.0.0",
  "description": "Lighthouse is a beautiful, yet simple to use tool to view how many Discord servers you are in",
  "main": "index.js",
  "scripts": {
    "watch:css": "NODE_ENV=development postcss public/css/index.css -o ./public/css/tailwind.css -w",
    "watch:scripts": "npx tsc -w",
    "build:scripts": "npx tsc",
    "build:css": "NODE_ENV=production postcss public/css/index.css -o public/css/tailwind.css",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeasideDevs/Lighthouse.git"
  },
  "author": "Issai H",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeasideDevs/Lighthouse/issues"
  },
  "homepage": "https://github.com/SeasideDevs/Lighthouse#readme",
  "devDependencies": {
    "express": "^4.17.1",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.4"
  }
}
